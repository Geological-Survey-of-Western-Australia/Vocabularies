PREFIX schema: <https://schema.org/>

<http://linked.data.gov.au/def/sample-location-status>
    schema:description "The status of a sample with respect to its location."@en ;
    schema:name "Sample Location Status"@en ;
.

<http://linked.data.gov.au/def/sample-location-status/collected>
    schema:description "Sample has been collected from a sampling location and is in temporary storage prior to transfer to permanent or semi-permanent storage"@en ;
    schema:name "Collected"@en ;
.

<http://linked.data.gov.au/def/sample-location-status/in-storage>
    schema:description "Held at a storage location in an archived state."@en ;
    schema:name "In Storage"@en ;
.

<http://linked.data.gov.au/def/sample-location-status/in-transit>
    schema:description "In transit between locations."@en ;
    schema:name "In transit"@en ;
.

<http://linked.data.gov.au/def/sample-location-status/lost>
    schema:description "Location of sample is not known"@en ;
    schema:name "Lost"@en ;
.

<http://linked.data.gov.au/def/sample-location-status/on-display>
    schema:description "Held in a storage facility, museum, or other location for display and/or analysis."@en ;
    schema:name "On Display"@en ;
.

<http://linked.data.gov.au/def/sample-location-status/processing>
    schema:description "The sample is at a laboratory, university, or other facility external to its registered storage facility undergoing processing or analysis."@en ;
    schema:name "Processing"@en ;
.
